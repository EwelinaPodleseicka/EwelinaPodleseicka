        if (a == 0)
            liniowe(b, c);
        else {
            double delta;
            delta = b * b - 4 * a * c;
            if (delta < 0)
                System.out.println("Brak rozwiązań");
            else if (delta == 0) {
                double x;
                x = -b / (2 * a);
                System.out.print("Jedno podwójne rozwiązanie x = ");
                System.out.println(x);
            } else {
                double x1, x2;
                x1 = (-b + sqrt(delta)) / (2 * a);
                x2 = (-b - sqrt(delta)) / (2 * a);
                System.out.print("x1 = ");
                System.out.println(x1);
                System.out.print("x2 = ");
                System.out.println(x2);
            }
        }
